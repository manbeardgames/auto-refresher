{
  "name": "auto-refresher",
  "version": "1.1.0",
  "description": "A jQuery plugin to auto-refresh a page, with a visual progress bar.",
  "keywords": [
    "jquery",
    "jquery-plugin",
    "plugins",
    "auto-refresher",
    "refresher"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manbeardgames/auto-refresher.git"
  },
  "bugs": {
    "url": "https://github.com/manbeardgames/auto-refresher/issues"
  },
  "author": {
    "name": "Christopher Whitley",
    "email": "project.dart.dev@gmail.com",
    "url": "https://github.com/manbeardgames"
  },
  "homepage": "https://github.com/manbeardgames/auto-refresher#readme",
  "contributors": [
    {
      "name": "Christopher Whitley",
      "email": "project.dart.dev@gmail.com",
      "url": "https://github.com/manbeardgames"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.4.1"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.12.0",
    "rimraf": "^3.0.0"
  },
  "scripts": {
    "build:dist": "mkdirp ./dist",
    "clean:dist": "rimraf ./dist",
    "clean": "npm run clean:dist",
    "eslint": "eslint src/js/",
    "build:css:min": "node-sass src/scss/autorefresher.scss dist/css/autorefresher.min.css --output-style compressed --source-map true",
    "build:css:expanded": "node-sass src/scss/autorefresher.scss dist/css/autorefresher.css --output-style expanded --source-map true",
    "build:css": "npm run build:css:expanded && npm run build:css:min",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
